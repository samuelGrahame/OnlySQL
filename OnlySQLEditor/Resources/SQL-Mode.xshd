        <Key word="remove" />
        <Key word="select" />
        <Key word="set" />
        <Key word="value" />
        <Key word="var" />
        <Key word="when" />
        <Key word="where" />
        <Key word="yield" />
      </KeyWords>
			<KeyWords name="SqlKeywordsBold" bold="false" italic="false" color="#0000FF">
				<Key word="BEGIN" />
				<Key word="END" />
				<Key word="EXEC" />
				<Key word="CREATE" />
				<Key word="COMMIT" />
        <Key word="REVOKE" />
        <Key word="RAISERROR" />
